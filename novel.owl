<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="http://www.example.org/ontology#"
     xml:base="http://www.example.org/ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:book="http://www.example.org/ontology#">

    <!-- Ontology Definition -->
    <owl:Ontology rdf:about="http://www.example.org/ontology"/>

    <!-- Classes -->
    <owl:Class rdf:about="http://www.example.org/ontology#Buku"/>
    <owl:Class rdf:about="http://www.example.org/ontology#Author"/>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="http://www.example.org/ontology#nama">
        <rdfs:domain rdf:resource="http://www.example.org/ontology#Buku"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.example.org/ontology#author">
        <rdfs:domain rdf:resource="http://www.example.org/ontology#Buku"/>
        <rdfs:range rdf:resource="http://www.example.org/ontology#Author"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.example.org/ontology#datepublished">
        <rdfs:domain rdf:resource="http://www.example.org/ontology#Buku"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.example.org/ontology#publisher">
        <rdfs:domain rdf:resource="http://www.example.org/ontology#Buku"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.example.org/ontology#isbn">
        <rdfs:domain rdf:resource="http://www.example.org/ontology#Buku"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Individuals (Books) -->
    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Buku1">
        <rdf:type rdf:resource="http://www.example.org/ontology#Buku"/>
        <book:nama>Laskar Pelangi</book:nama>
        <book:author rdf:resource="http://www.example.org/ontology#Author1"/>
        <book:datepublished>2005</book:datepublished>
        <book:publisher>Gramedia</book:publisher>
        <book:isbn>ISBN-9789792242703</book:isbn>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Buku2">
        <rdf:type rdf:resource="http://www.example.org/ontology#Buku"/>
        <book:nama>Perahu Kertas</book:nama>
        <book:author rdf:resource="http://www.example.org/ontology#Author2"/>
        <book:datepublished>2009</book:datepublished>
        <book:publisher>Gramedia Pustaka Utama</book:publisher>
        <book:isbn>ISBN-9789792736237</book:isbn>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Buku3">
        <rdf:type rdf:resource="http://www.example.org/ontology#Buku"/>
        <book:nama>Harry Potter and the Sorcerer's Stone</book:nama>
        <book:author rdf:resource="http://www.example.org/ontology#Author3"/>
        <book:datepublished>1997</book:datepublished>
        <book:publisher>Scholastic</book:publisher>
        <book:isbn>ISBN-9780439708180</book:isbn>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Buku4">
        <rdf:type rdf:resource="http://www.example.org/ontology#Buku"/>
        <book:nama>1984</book:nama>
        <book:author rdf:resource="http://www.example.org/ontology#Author4"/>
        <book:datepublished>1949</book:datepublished>
        <book:publisher>Secker dan Warburg</book:publisher>
        <book:isbn>ISBN-9780451524935</book:isbn>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Buku5">
        <rdf:type rdf:resource="http://www.example.org/ontology#Buku"/>
        <book:nama>The Great Gatsby</book:nama>
        <book:author rdf:resource="http://www.example.org/ontology#Author5"/>
        <book:datepublished>1925</book:datepublished>
        <book:publisher>Charles Scribner's Sons</book:publisher>
        <book:isbn>ISBN-9780743273565</book:isbn>
    </owl:NamedIndividual>

    <!-- Individuals (Authors) -->
    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Author1">
        <rdf:type rdf:resource="http://www.example.org/ontology#Author"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Author2">
        <rdf:type rdf:resource="http://www.example.org/ontology#Author"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Author3">
        <rdf:type rdf:resource="http://www.example.org/ontology#Author"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Author4">
        <rdf:type rdf:resource="http://www.example.org/ontology#Author"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.example.org/ontology#Author5">
        <rdf:type rdf:resource="http://www.example.org/ontology#Author"/>
    </owl:NamedIndividual>

</rdf:RDF>
