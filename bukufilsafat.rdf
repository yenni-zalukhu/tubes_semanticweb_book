    </owl:NamedIndividual>
    


    <!-- http://www.tubessemantic.id/ws/bukufilsafat.owl#Filsafat_sejarah_:_profetik,_spekulatif,_dan_kritis -->

    <owl:NamedIndividual rdf:about="http://www.tubessemantic.id/ws/bukufilsafat.owl#Filsafat_sejarah_:_profetik,_spekulatif,_dan_kritis">
