    <owl:NamedIndividual rdf:about="http://www.tubessemantic.id/ws/bukufilsafat.owl#Filsafat_sejarah">
        <rdf:type rdf:resource="http://www.tubessemantic.id/ws/bukufilsafat.owl#filsafat_kebudayaan"/>
